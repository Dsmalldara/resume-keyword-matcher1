{
  "name": "resume-keyword-matcher-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "generate:openapi": "node ./scripts/generate-openapi.cjs",
    "generate:types": "npx openapi-typescript ./public/openapi.json --output ./src/types/api.ts",
    "prepush:check": "pnpm generate:openapi && pnpm generate:types && git diff --exit-code || (echo 'Generated API artifacts out of date' && exit 1)",
    "test": "jest --passWithNoTests",
  "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@sentry/node": "^10.13.0",
    "@supabase/supabase-js": "^2.57.4",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-pino-logger": "^7.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pino": "^9.11.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-pino-logger": "^4.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "eslint": "^9.36.0",
    "husky": "^8.0.0",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "commitizen": {
    "path": "./node_modules/cz-conventional-changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm prepush:check"
    }
  }
}
